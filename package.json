    "drizzle-kit": "^0.28.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "tsx": "^4.19.2",
    "vite": "^5.0.3",
    "vitest": "^2.1.5"
  },
  "resolutions": {
