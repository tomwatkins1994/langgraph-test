    "drizzle-orm": "^0.36.3",
    "langchain": "^0.3.5",
    "pdf-parse": "^1.1.1",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
